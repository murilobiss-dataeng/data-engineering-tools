# Exemplo de arquivo .env (opcional)
# Este arquivo pode ser usado para variáveis de ambiente adicionais

# Diretório de saída dos vídeos
OUTPUT_DIR=outputs

# ID da liga de futebol (padrão: 71 = Brasileirão)
FOOTBALL_LEAGUE_ID=71

# Idioma para TTS (pt = português, en = inglês)
TTS_LANGUAGE=pt

# Velocidade do TTS (False = normal, True = lento)
TTS_SLOW=False
